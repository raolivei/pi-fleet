{
  "title": "iPhone Export E-commerce",
  "uid": "iphone-export-dashboard",
  "tags": ["eldertree", "iphone-export", "ecommerce", "applications"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "editable": true,
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "type": "row",
      "title": "Service Health",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "collapsed": false
    },
    {
      "id": 2,
      "type": "stat",
      "title": "API",
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 1 },
      "targets": [
        {
          "expr": "count(kube_pod_status_phase{namespace=\"iphone-export\", pod=~\"iphone-export-api-.*\", phase=\"Running\"})",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [
            { "type": "value", "options": { "1": { "color": "green", "text": "UP" } } },
            { "type": "special", "options": { "match": "null", "result": { "color": "red", "text": "DOWN" } } }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "green", "value": 1 }
            ]
          }
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center"
      }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Frontend",
      "gridPos": { "h": 4, "w": 4, "x": 4, "y": 1 },
      "targets": [
        {
          "expr": "count(kube_pod_status_phase{namespace=\"iphone-export\", pod=~\"iphone-export-frontend-.*\", phase=\"Running\"})",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [
            { "type": "value", "options": { "1": { "color": "green", "text": "UP" } } },
            { "type": "special", "options": { "match": "null", "result": { "color": "red", "text": "DOWN" } } }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "green", "value": 1 }
            ]
          }
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center"
      }
    },
    {
      "id": 4,
      "type": "stat",
      "title": "PostgreSQL",
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 1 },
      "targets": [
        {
          "expr": "count(kube_pod_status_phase{namespace=\"iphone-export\", pod=~\"iphone-export-postgres-.*\", phase=\"Running\"})",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [
            { "type": "value", "options": { "1": { "color": "green", "text": "UP" } } },
            { "type": "special", "options": { "match": "null", "result": { "color": "red", "text": "DOWN" } } }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "green", "value": 1 }
            ]
          }
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center"
      }
    },
    {
      "id": 5,
      "type": "stat",
      "title": "Redis",
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 1 },
      "targets": [
        {
          "expr": "count(kube_pod_status_phase{namespace=\"iphone-export\", pod=~\"iphone-export-redis-.*\", phase=\"Running\"})",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [
            { "type": "value", "options": { "1": { "color": "green", "text": "UP" } } },
            { "type": "special", "options": { "match": "null", "result": { "color": "red", "text": "DOWN" } } }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "green", "value": 1 }
            ]
          }
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center"
      }
    },
    {
      "id": 6,
      "type": "stat",
      "title": "Restarts (1h)",
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 1 },
      "targets": [
        {
          "expr": "sum(increase(kube_pod_container_status_restarts_total{namespace=\"iphone-export\"}[1h]))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 3 }
            ]
          }
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area"
      }
    },
    {
      "id": 7,
      "type": "stat",
      "title": "Total Pods",
      "gridPos": { "h": 4, "w": 4, "x": 20, "y": 1 },
      "targets": [
        {
          "expr": "count(kube_pod_status_phase{namespace=\"iphone-export\", phase=\"Running\"})",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "fixed", "fixedColor": "blue" }
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area"
      }
    },
    {
      "id": 8,
      "type": "row",
      "title": "API Metrics",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 5 },
      "collapsed": false
    },
    {
      "id": 9,
      "type": "timeseries",
      "title": "API Request Rate",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 6 },
      "targets": [
        {
          "expr": "sum(rate(traefik_service_requests_total{namespace=\"iphone-export\", service=~\"iphone-export-api.*\"}[5m])) by (code)",
          "legendFormat": "HTTP {{code}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "reqps",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 20,
            "gradientMode": "scheme",
            "lineInterpolation": "smooth"
          }
        }
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "right", "showLegend": true },
        "tooltip": { "mode": "multi", "sort": "desc" }
      }
    },
    {
      "id": 10,
      "type": "timeseries",
      "title": "API Response Time",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 6 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(traefik_service_request_duration_seconds_bucket{namespace=\"iphone-export\", service=~\"iphone-export-api.*\"}[5m])) by (le))",
          "legendFormat": "p95",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.50, sum(rate(traefik_service_request_duration_seconds_bucket{namespace=\"iphone-export\", service=~\"iphone-export-api.*\"}[5m])) by (le))",
          "legendFormat": "p50",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "s",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10,
            "lineInterpolation": "smooth"
          }
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "id": 11,
      "type": "row",
      "title": "Resource Usage",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 14 },
      "collapsed": false
    },
    {
      "id": 12,
      "type": "timeseries",
      "title": "CPU Usage",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 15 },
      "targets": [
        {
          "expr": "sum(rate(container_cpu_usage_seconds_total{namespace=\"iphone-export\", container!=\"\", container!=\"POD\"}[5m])) by (pod)",
          "legendFormat": "{{pod}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 20,
            "gradientMode": "scheme",
            "lineInterpolation": "smooth"
          }
        }
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "right", "showLegend": true },
        "tooltip": { "mode": "multi", "sort": "desc" }
      }
    },
    {
      "id": 13,
      "type": "timeseries",
      "title": "Memory Usage",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 15 },
      "targets": [
        {
          "expr": "sum(container_memory_working_set_bytes{namespace=\"iphone-export\", container!=\"\", container!=\"POD\"}) by (pod)",
          "legendFormat": "{{pod}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "bytes",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 20,
            "gradientMode": "scheme",
            "lineInterpolation": "smooth"
          }
        }
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "right", "showLegend": true },
        "tooltip": { "mode": "multi", "sort": "desc" }
      }
    },
    {
      "id": 14,
      "type": "row",
      "title": "Storage",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 23 },
      "collapsed": false
    },
    {
      "id": 15,
      "type": "gauge",
      "title": "PostgreSQL PVC Usage",
      "gridPos": { "h": 6, "w": 8, "x": 0, "y": 24 },
      "targets": [
        {
          "expr": "kubelet_volume_stats_used_bytes{namespace=\"iphone-export\", persistentvolumeclaim=~\".*postgres.*\"} / kubelet_volume_stats_capacity_bytes{namespace=\"iphone-export\", persistentvolumeclaim=~\".*postgres.*\"} * 100",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 70 },
              { "color": "red", "value": 85 }
            ]
          }
        }
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "showThresholdMarkers": true
      }
    },
    {
      "id": 16,
      "type": "timeseries",
      "title": "Storage Used Over Time",
      "gridPos": { "h": 6, "w": 16, "x": 8, "y": 24 },
      "targets": [
        {
          "expr": "kubelet_volume_stats_used_bytes{namespace=\"iphone-export\"}",
          "legendFormat": "{{persistentvolumeclaim}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "bytes",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 20,
            "gradientMode": "scheme"
          }
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "multi" }
      }
    }
  ]
}
