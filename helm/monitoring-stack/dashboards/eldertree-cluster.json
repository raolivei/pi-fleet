            ]
          }
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center"
      }
    },
    {
      "id": 6,
      "type": "stat",
      "title": "Vault HA Status",
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 1 },
      "targets": [
        {
          "expr": "sum(kube_pod_status_phase{namespace=\"vault\", pod=~\"vault-.*\", phase=\"Running\"})",
          "legendFormat": "Vault Pods",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [
            { "type": "value", "options": { "3": { "color": "green", "text": "HA (3/3)" } } },
            { "type": "value", "options": { "2": { "color": "yellow", "text": "Degraded (2/3)" } } },
            { "type": "value", "options": { "1": { "color": "orange", "text": "Critical (1/3)" } } }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "orange", "value": 1 },
              { "color": "yellow", "value": 2 },
              { "color": "green", "value": 3 }
            ]
          }
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center"
      }
    },
    {
      "id": 7,
      "type": "stat",
      "title": "Total Pods",
      "gridPos": { "h": 4, "w": 4, "x": 20, "y": 1 },
      "targets": [
        {
          "expr": "sum(kube_pod_status_phase{phase=\"Running\"})",
          "legendFormat": "Running",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "fixed", "fixedColor": "blue" }
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "center"
      }
    },
    {
      "id": 8,
      "type": "row",
      "title": "Resource Overview",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 5 },
      "collapsed": false
    },
    {
      "id": 9,
      "type": "gauge",
      "title": "Cluster CPU Usage",
      "gridPos": { "h": 6, "w": 6, "x": 0, "y": 6 },
      "targets": [
        {
          "expr": "100 - (avg(rate(node_cpu_seconds_total{mode=\"idle\"}[5m])) * 100)",
          "legendFormat": "CPU",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 70 },
              { "color": "red", "value": 85 }
            ]
          }
        }
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "showThresholdMarkers": true,
        "showThresholdLabels": false
      }
    },
    {
      "id": 10,
      "type": "gauge",
      "title": "Cluster Memory Usage",
      "gridPos": { "h": 6, "w": 6, "x": 6, "y": 6 },
      "targets": [
        {
          "expr": "(1 - (sum(node_memory_MemAvailable_bytes) / sum(node_memory_MemTotal_bytes))) * 100",
          "legendFormat": "Memory",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 75 },
              { "color": "red", "value": 90 }
            ]
          }
        }
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "showThresholdMarkers": true
      }
    },
    {
      "id": 11,
      "type": "gauge",
      "title": "Root Disk Usage",
      "gridPos": { "h": 6, "w": 6, "x": 12, "y": 6 },
      "targets": [
        {
          "expr": "avg((1 - node_filesystem_avail_bytes{mountpoint=\"/\"} / node_filesystem_size_bytes{mountpoint=\"/\"}) * 100)",
          "legendFormat": "Disk",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 70 },
              { "color": "red", "value": 85 }
            ]
          }
        }
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "showThresholdMarkers": true
      }
    },
    {
      "id": 12,
      "type": "gauge",
      "title": "Avg Node Temperature",
      "gridPos": { "h": 6, "w": 6, "x": 18, "y": 6 },
      "targets": [
        {
          "expr": "avg(node_hwmon_temp_celsius)",
          "legendFormat": "Temp",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "unit": "celsius",
          "min": 30,
          "max": 90,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 65 },
              { "color": "red", "value": 75 }
            ]
          }
        }
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "showThresholdMarkers": true
      }
    },
    {
      "id": 13,
      "type": "row",
      "title": "Namespace Overview",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 12 },
      "collapsed": false
    },
    {
      "id": 14,
      "type": "bargauge",
      "title": "Pods by Namespace",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 13 },
      "targets": [
        {
          "expr": "sum(kube_pod_status_phase{phase=\"Running\"}) by (namespace)",
          "legendFormat": "{{namespace}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "thresholds": {
            "mode": "absolute",
            "steps": [{ "color": "green", "value": null }]
          }
        }
      },
      "options": {
        "displayMode": "gradient",
        "orientation": "horizontal",
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "showUnfilled": true
      }
    },
    {
      "id": 15,
      "type": "bargauge",
      "title": "Memory by Namespace",
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 13 },
      "targets": [
        {
          "expr": "sum(container_memory_working_set_bytes{container!=\"\", container!=\"POD\"}) by (namespace)",
          "legendFormat": "{{namespace}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "bytes",
          "thresholds": {
            "mode": "absolute",
            "steps": [{ "color": "green", "value": null }]
          }
        }
      },
      "options": {
        "displayMode": "gradient",
        "orientation": "horizontal",
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "showUnfilled": true
      }
    },
    {
      "id": 16,
      "type": "bargauge",
      "title": "CPU by Namespace",
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 13 },
      "targets": [
        {
          "expr": "sum(rate(container_cpu_usage_seconds_total{container!=\"\", container!=\"POD\"}[5m])) by (namespace) * 100",
          "legendFormat": "{{namespace}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
