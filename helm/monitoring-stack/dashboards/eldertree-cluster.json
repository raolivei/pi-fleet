{
  "title": "Eldertree Cluster Overview",
  "uid": "eldertree-cluster",
  "tags": ["eldertree", "cluster", "ha", "overview"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "editable": true,
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "type": "row",
      "title": "Cluster Health (3-Node HA)",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "collapsed": false
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Nodes Ready",
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 1 },
      "targets": [
        {
          "expr": "sum(kube_node_status_condition{condition=\"Ready\", status=\"true\"})",
          "legendFormat": "Ready",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 2 },
              { "color": "green", "value": 3 }
            ]
          },
          "mappings": []
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center",
        "textMode": "value_and_name"
      }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Node 1",
      "gridPos": { "h": 4, "w": 4, "x": 4, "y": 1 },
      "targets": [
        {
          "expr": "kube_node_status_condition{node=\"node-1\", condition=\"Ready\", status=\"true\"}",
          "legendFormat": "node-1",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [
            { "type": "value", "options": { "1": { "color": "green", "text": "READY" } } },
            { "type": "special", "options": { "match": "null", "result": { "color": "red", "text": "DOWN" } } }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "green", "value": 1 }
            ]
          }
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center"
      }
    },
    {
      "id": 4,
      "type": "stat",
      "title": "Node 2",
